  },
  "repository": "https://github.com/Apex-Aimer/monorepo/apps/blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:content\" \"next dev --port 3001\"",
    "dev:content": "npx nodemon --watch posts -e mdx --exec \"npx ts-node ./scripts/processPosts.ts\"",
    "build": "yarn build:content && next build",
    "build:content": "npx ts-node ./scripts/processPosts.ts",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.9",
    "clsx": "^1.2.1",
    "contentlayer": "0.3.1",
    "date-fns": "^2.30.0",
    "next": "^13.4.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ui": "*"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.2.0",
    "@types/mdx": "^2.0.5",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "concurrently": "^8.2.0",
    "eslint-config-custom": "*",
    "tailwind-custom": "*",
    "tsconfig": "*",
    "typescript": "^4.5.3",
    "vercel": "^31.0.1"
  }
