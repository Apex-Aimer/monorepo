{
  "name": "ApexAimerCMS",
  "description": "CMS for ApexAimer",
  "author": {
    "name": "Aleksei Savelev",
    "url": "https://github.com/savelichalex"
  },
  "repository": "https://github.com/Apex-Aimer/monorepo/apps/cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "yarn pages:build && wrangler pages dev .vercel/output/static",
    "deploy": "yarn pages:build && wrangler pages deploy .vercel/output/static",
    "up:dev": "wrangler d1 migrations apply $PROJECT_D1_NAME --local",
    "up:prod": "wrangler d1 migrations apply $PROJECT_D1_NAME",
    "update_db:dev": "npx wrangler d1 execute $PROJECT_D1_NAME --local --file=./scratchbook.sql"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "@cloudflare/next-on-pages": "^1.9.0",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@uppy/core": "^3.9.3",
    "@uppy/drag-drop": "^3.0.3",
    "@uppy/progress-bar": "^3.1.0",
    "@uppy/react": "^3.2.2",
    "antd": "^5.14.2",
    "arctic": "^1.2.1",
    "clsx": "^2.1.0",
    "kysely": "^0.27.2",
    "kysely-d1": "^0.3.0",
    "lodash": "^4.17.21",
    "lucia": "^3.0.1",
    "next": "^14",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.2.5",
    "ui": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint-config-custom": "*",
    "tailwind-custom": "*",
    "tsconfig": "*",
    "typescript": "^4.5.3",
    "vercel": "^33.5.3",
    "wrangler": "^3.30.0"
  }
}
